//在x和y轴上投影，观察重叠和不重叠有什么规律
class Solution {
    public boolean isRectangleOverlap(int[] rec1, int[] rec2) {
        if (Math.max(rec1[0], rec2[0]) < Math.min(rec1[2], rec2[2]) &&
                Math.max(rec1[1], rec2[1]) < Math.min(rec1[3], rec2[3])) {
            return true;
        } else {
            return false;
        }
    }
}
