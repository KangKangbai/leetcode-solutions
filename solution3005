//这里用到HashMap保存
class Solution {
    public int maxFrequencyElements(int[] nums) {
        Map<Integer, Integer> map = new HashMap<>();
        for(int num : nums) {
            map.put(num, map.getOrDefault(num, 0) + 1);
        }
        int maxFre = 0;
        for(int fre : map.values()) {
            if(fre > maxFre) {
                maxFre = fre;
            }
        }
        int res = 0;
        for(int count : map.values()) {
            if(count == maxFre) {
                res += count;
            }
        }
        return res;
    }
}
