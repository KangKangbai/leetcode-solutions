//不用先排序，会比较耗时
class Solution {
    public int minOperations(int[] nums, int k) {
//        Arrays.sort(nums);
//        if(k > nums[0]) return -1;

        HashSet<Integer> integers = new HashSet<>();
        for(int i = 0; i < nums.length; i++) {
            if(nums[i] < k) return -1;
            if(nums[i] > k)
            integers.add(nums[i]);
        }
        int res = integers.size();
        return res;
    }
}
